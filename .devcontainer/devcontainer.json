{
  "hostRequirements": {
    "cpus": 4
  },
  "waitFor": "onCreateCommand",
  "updateContentCommand": "npm install",
  "postCreateCommand": "cd react-app/",
  "postAttachCommand": {
    "server": "npm run dev"
  },
  "customizations": {
    "codespaces": {
      "openFiles": [
        "src/App.js"
      ]
    }
  },
  "portsAttributes": {
    "5173": {
      "label": "Image Compression App",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [5173]
}
